{
  "name": "qantas-code-test",
  "version": "1.0.0",
  "description": "Qantas Code Test",
  "private": true,
  "scripts": {
    "start:dev": "webpack-dev-server",
    "prestart:prod": "npm run build",
    "start:test": "PORT=8080 npm run start:prod",
    "start:prod": "serve -s build",
    "test:unit": "jest src --config ./jest.config.json --coverage --coverageReporters=text",
    "test:snap": "jest storyshots --config ./jest.config.json",
    "test:snapUpdate": "npm run test:snap -- -u",
    "test:e2e": "jest e2e --runInBand --preset jest-puppeteer --verbose",
    "test": "npm run test:snap && npm run test:unit && npm run test:e2e",
    "pretest": "npm run lint",
    "build": "webpack --mode production",
    "lint": "eslint --ext .jsx,.js ./src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy": "npm run test && npm run build",
    "deploy": "gh-pages -d build"
  },
  "author": {
    "name": "Long Zhao",
    "email": "zlong@outlook.com",
    "url": "https://www.linkedin.com/in/long-zhao-32a96916a/"
  },
  "engines": {
    "node": ">=10.13.0",
    "npm": ">=6.7.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "styled-components": "^3.4.10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.3",
    "@storybook/addon-knobs": "^5.1.3",
    "@storybook/addon-links": "^5.1.3",
    "@storybook/addon-storyshots": "^5.1.3",
    "@storybook/addons": "^5.1.3",
    "@storybook/react": "^5.1.3",
    "@testing-library/react": "^8.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^4.0.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.5.0",
    "jest-puppeteer": "^4.2.0",
    "jest-styled-components": "^6.3.1",
    "jest-transform-stub": "^2.0.0",
    "puppeteer": "^1.17.0",
    "react-test-renderer": "^16.8.6",
    "serve": "^11.0.1",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  }
}
